{
  "name": "react-bootstrap-accordion",
  "version": "0.0.0-development",
  "description": "A simple react component for adding a nice typewriter effect to your project.",
  "author": "awran5 <awran5@yahoo.com>",
  "license": "MIT",
  "repository": "https://github.com/awran5/react-bootstrap-accordion",
  "bugs": "https://github.com/awran5/react-bootstrap-accordion/issues",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "source": "src/index.tsx",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "clean": "rm -rf dist",
    "prepare": "rollup -c --exports auto && cd example && yarn",
    "start": "concurrently \"rollup -cw --exports auto\" \"cd example && yarn start\"",
    "build": "yarn clean && rollup -c --exports auto",
    "predeploy": "cd example && yarn && yarn build",
    "deploy": "gh-pages -d example/build",
    "commit": "git cz",
    "release": "semantic-release"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/node": "^16.3.1",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "autoprefixer": "^10.3.0",
    "commitizen": "^4.2.4",
    "concurrently": "^6.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.2.3",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "rollup": "^2.53.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "semantic-release": "^17.4.4",
    "typescript": "^4.3.5"
  },
  "dependencies": {},
  "keywords": [],
  "tags": [],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
